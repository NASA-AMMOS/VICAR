process help=*
parm inp type=string count=2
parm out type=string count=2
parm filter type=keyword count=0:1 valid=(filter,nofilter) default=nofilter
parm fnlw type=integer count=1 valid=1:25 default=3
parm fnsw type=integer count=1 valid=1:25 default=3
!horizon location
parm avgnw type=integer count=0:1 valid=1:15 default=9
parm actnw type=integer count=0:1 valid=1:255 default=9
parm skyact type=real count=1 default=100.
parm maxpatch type=integer count=1 default=400
parm hthresh type=real count=1 default=1.0
!image matching
parm nlw type=integer count=1 valid=1:51 default=7
parm nsw type=integer count=1 valid=1:51 default=11
parm rdn     type=real count=1 default=10000.
parm cthresh type=real count=1 valid=1.:999999. default=10000.
parm iz type=integer count=2 valid=1:3 default=(3,3)
parm ic type=integer count=2 valid=-4:4 default=(-4,4)
parm angle type=integer count=3 valid=-180:359 default=(0,0,10)
parm dl type=integer count=1 valid=3:15 default=5
parm ds type=integer count=1 valid=3:15 default=5
parm sfactor type=real count=1 default=2.5
parm rthresh type=real count=1 default=.3
parm sthresh type=real count=1 valid=0.:99999. default=10.
parm qthresh type=real count=1 valid=-1.:1. default=.7
parm dxthresh type=integer count=1 valid=1:99999 default=200
parm dythresh type=integer count=1 valid=1:99999 default=50
parm minpts type=integer count=1 valid=10:2048 default=10
parm tangle type=real count=1 default=0.
parm tscale type=real count=1 default=1.
parm avg type=string count=0:2 default=""
parm sig type=string count=0:2 default=""
parm activity type=string count=0:2 default=""
parm horizon type=string count=0:2 default=""
parm cdisp   type=string count=0:2 default=""
parm ctpdisp type=string count=0:2 default=""
parm tdisp   type=string count=0:2 default=""
parm marsout type=string count=0:2 default=""
parm debug type=keyword count=1 valid=(debug,nodebug) default=nodebug
parm print type=keyword count=1 valid=(print,noprint) default=noprint
end-proc
