procedure
!refgbl $autousage
body
!let $autousage="none"

enable-log

! In ported code this should also work for the
! files generated by the ported ibisgr library.
!
ibis-gen graph 'row nc=3 nr=9 datacol=(1,2,3) +
   data=( 1,1,5,   1,8,7,   3,8,20, +
   		  3,1,30,  8,2,8,   8,8,254, +
		  9,9,500, 10,10,1234567,  1000,1000,4)
ibis-list graph gr1dim=3 nr=9

!Test the various formatting options
xyzpic2 graph image nl=8 ns=8
list image 'nousrtim

xyzpic2 graph image nl=9 ns=9 format=half
list image 'nousrtim

xyzpic2 graph image nl=10 ns=10 format=full
list image 'nousrtim

xyzpic2 graph image nl=10 ns=10 format=real
list image 'nousrtim

disable-log

ush rm graph image

end-proc
