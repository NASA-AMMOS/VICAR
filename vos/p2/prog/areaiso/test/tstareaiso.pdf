procedure
refgbl $echo
body
enable-log

let $echo="yes"
!
! for phobos
areaiso out= +
 (area_lat.img,area_lon.img,area_map.img,angle_map.img,del_area_lat.img, +
 del_area_lon.img) +
 planet=phobos 'north path=.

list area_lat.img     'nousrtim 
list area_lon.img     'nousrtim 
list area_map.img     'nousrtim 
list angle_map.img    'nousrtim 
list del_area_lat.img 'nousrtim 
list del_area_lon.img 'nousrtim 

!concat inp=(area_lat.img,area_lon.img,area_map.img,angle_map.img, +
! del_area_lat.img,del_area_lon.img) out=auth_mos_cheng.img ns=146
!size inp=auth_mos_cheng.img out=a.img zoom=4
!xvd a.img
!barne_r inp=a.img miplbox=21 primary=42057
!areaiso out= +
! (area_lat.img,area_lon.img,area_map.img,angle_map.img,del_area_lat.img, +
! del_area_lon.img) +
! planet=phobos 'south
!concat inp=(area_lat.img,area_lon.img,area_map.img,angle_map.img, +
! del_area_lat.img,del_area_lon.img) out=auth_mos_cheng.img ns=146
!size inp=auth_mos_cheng.img out=a.img zoom=4
!xvd a.img
!barne_r inp=a.img miplbox=21 primary=42057
!
! for triaxial
!areaiso out= +
! (area_lat.img,area_lon.img,area_map.img,angle_map.img,del_area_lat.img, +
! del_area_lon.img) +
! planet=phobos triaxial=(13.1,11.2,9.2) 'north
!concat inp=(area_lat.img,area_lon.img,area_map.img,angle_map.img, +
! del_area_lat.img,del_area_lon.img) out=auth_mos_cheng.img ns=146
!size inp=auth_mos_cheng.img out=a.img zoom=4
!xvd a.img
!barne_r inp=a.img miplbox=21 primary=42057
!areaiso out= +
! (area_lat.img,area_lon.img,area_map.img,angle_map.img,del_area_lat.img, +
! del_area_lon.img) +
! planet=phobos triaxial=(13.1,11.2,9.2) 'south
!concat inp=(area_lat.img,area_lon.img,area_map.img,angle_map.img, +
! del_area_lat.img,del_area_lon.img) out=auth_mos_cheng.img ns=146
!size inp=auth_mos_cheng.img out=a.img zoom=4
!xvd a.img
!barne_r inp=a.img miplbox=21 primary=42057
!

ush rm angle_map.img
ush rm area_lat.img
ush rm area_lon.img
ush rm area_map.img
ush rm del_area_lat.img
ush rm del_area_lon.img

disable-log
end-proc
